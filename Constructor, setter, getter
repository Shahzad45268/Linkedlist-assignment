// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class Node 
{
    private:
    int data;
    Node*next;
    public:
    Node()
    {
        data=0;
        next=NULL;
        
    }
    Node(int data)
    {
        this->data=data;
        next=NULL;
    } 
    

   int getData()
   {
       return this->data;
   }
   void setData(int data)
   {
     this->data=data;
   } 
   Node*getNext()
   {
       return next;
   }
   void setNext(Node*next)
   {
       this->next=next;
   } 
  };
   int main()
   {
       Node n1(1);
       Node n2(2);
       n1.setNext(&n2);
   }
